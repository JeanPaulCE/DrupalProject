    <div class="card">
        
        <div class="image-card">{{ image }}</div>
        <div class="card-content">
            <p class='date'>{{date}} </p>
            
            <a href="{{ url }}" class="link-container">
            <h3>{{text_url}}</h3>
            <i class="fa-solid fa-arrow-up-right-from-square"></i>
            </a>

            <div class="card-description"><p>{{description}} </p></div>
            
        </div>
        <div class="tags-post">
            <ul>
                <p>{{tag0}}</p>
                <p>{{tag1}}</p>
                <p>{{tag2}}</p>
            </ul>
        </div>
    </div>